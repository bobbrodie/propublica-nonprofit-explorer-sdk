{
  "name": "propublica-nonprofit-explorer-sdk",
  "version": "1.0.0",
  "description": "SDK for ProPublica Nonprofit Explorer API",
  "main": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": "./dist/index.js",
    "./models": "./dist/models/index.js",
    "./constants": "./dist/constants/index.js"
  },
  "repository": "bobbrodie/propublica-nonprofit-explorer-sdk",
  "scripts": {
    "build": "rimraf dist && tsc -p ./tsconfig.json",
    "coverage": "npm run unit -- --coverage",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "test": "npm run format && npm run lint && npm run coverage",
    "unit": "jest --runInBand"
  },
  "keywords": [
    "nonprofit"
  ],
  "author": "Robert Brodie <rbrodie85@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18 <24"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.14",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
