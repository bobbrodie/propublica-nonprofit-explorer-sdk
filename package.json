{
  "name": "propublica-nonprofit-explorer-sdk",
  "version": "3.1.0",
  "description": "SDK for ProPublica Nonprofit Explorer API",
  "main": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": "./dist/index.js",
    "./constants": "./dist/constants/index.js",
    "./models": "./dist/models/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobbrodie/propublica-nonprofit-explorer-sdk.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p ./tsconfig.json",
    "coverage": "npm run unit -- --coverage",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky",
    "test": "npm run format && npm run lint && npm run coverage",
    "unit": "jest --runInBand"
  },
  "keywords": [
    "nonprofit"
  ],
  "author": "Robert Brodie <rbrodie85@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20 <25"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/jest": "30.0.0",
    "eslint": "10.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "rimraf": "6.1.3",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0"
  },
  "dependencies": {
    "zod": "4.3.6"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write ."
    ]
  }
}
